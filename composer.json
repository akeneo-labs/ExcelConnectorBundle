{
    "name": "akeneo/excel-connector-bundle",
    "type": "symfony-bundle",
    "description": "Akeneo PIM Excel connector bundle",
    "keywords": ["pim", "akeneo", "ecommerce", "e-commerce", "excel", "connector"],
    "homepage": "http://akeneo.com",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Antoine Guigan",
            "email": "antoine@akeneo.com",
            "homepage": "http://akeneo.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/platform.git"
        }
    ],
    "require": {
        "akeneo/pim-community-dev": "1.4.*",
        "akeneo/batch-bundle": "0.4.x-dev",
        "akeneo/measure-bundle": "0.4.x-dev",
        "oro/platform": "dev-pim-1.4",
        "doctrine/migrations": "1.0.*@dev",
        "phpoffice/phpexcel": "1.8.*",
        "akeneo/spreadsheet-parser-bundle": "1.1.*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Pim\\Bundle\\ExcelConnectorBundle\\": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Pim/Bundle/ExcelConnectorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5.x-dev"
        }
    }
}
